{
    "name": "symfony-si/symfony-si",
    "license": "MIT",
    "type": "project",
    "description": "Slovenian Symfony user group application",
    "keywords": ["symfony", "php", "project", "group"],
    "homepage": "https://symfony.si",
    "authors": [
        {
            "name": "Peter Kokot",
            "email": "peterkokot@gmail.com",
            "homepage": "https://m-m.si",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "symfony/symfony": "3.1.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/github-api": "^1.7",
        "php-pm/php-pm": "dev-master",
        "php-pm/httpkernel-adapter": "dev-master",
        "mnapoli/front-yaml": "^1.5",
        "symfony-si/symfony-cheatsheet": "*",
        "symfony-si/symfony-resources": "*",
        "symfony-si/fig-standards-sl": "*",
        "symfony-si/conduct": "*"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "repositories": {
        "cheatsheet": {
            "type": "package",
            "package": {
                "name": "symfony-si/symfony-cheatsheet",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/symfony-si/symfony-cheatsheet",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "resources": {
            "type": "package",
            "package": {
                "name": "symfony-si/symfony-resources",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/symfony-si/symfony-resources",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "psrs": {
            "type": "package",
            "package": {
                "name": "symfony-si/fig-standards-sl",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/symfony-si/fig-standards-sl",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "conduct": {
            "type": "package",
            "package": {
                "name": "symfony-si/conduct",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/symfony-si/conduct",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
